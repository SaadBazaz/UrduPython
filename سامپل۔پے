کچھ = ۰
چلو = حق
جبتک (کچھ <= ۱۰۰):
    چھاپ("رکن #"، کچھ)
    اگر کچھ%۲۰ == ۰:
       جواب = داخله("کیا آپ ختم کرنا چاھتے ہیں؟ (ج/ن)")
       اگر جواب == "ج":
            چلو = باطل
            توڑ 
       ورنہ اگر جواب == "ن":
            چھاپ("آگے چلتے ہیں!")     
       ورنہ:
            چھاپ("سمجھ نہیں آئ۔۔۔")
            جاری
    کچھ += ۱

اگر چلو ہے باطل:
    چھاپ("اللہ ہافظ!")     
ورنہ: چھاپ("اختتام۔")