کچھ = ۱۰۰

جبتک (کچھ >= ۰):
    چھاپ("رکن #"، کچھ)
    اگر کچھ == ۵۰:
       جواب = داخله("کیا آپ ختم کرنا چاھتے ھیں؟ (ج/ن)")
       اگر جواب == "ج":
            چھاپ("اللہ ہافظ!")     
            توڑ 
       ورنہ اگر جواب == "ن":
            چھاپ("آگے چلتے ہیں!")     
       ورنہ:
            چھاپ("سمجھ نہیں آئ۔۔۔")
            جاری
    کچھ -= ۱